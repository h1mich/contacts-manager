{
  "name": "contacts-manager",
  "version": "1.0.0",
  "description": "A very simple contacts manager",
  "main": "entry-point.js",
  "scripts": {
    "client:dev:once": "webpack -d",
    "client:dev:watch": "webpack -d --watch --progress",
    "client:prod": "webpack -p",
    "client:lint": "eslint ./entry-point.jsx ./components/common/** ./components/** || exit 0",
    "server:dev:once": "node --inspect http-server.js",
    "server:dev:watch": "nodemon --ignore dist/bundle.js --inspect http-server.js",
    "server:prod": "node http-server.js",
    "server:lint": "eslint ./http-server.js ./api/** ./api/middlewares/** || exit 0"
  },
  "author": "Egor Koshelko",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "browser-sync": "^2.24.5",
    "cookie-parser": "^1.4.3",
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "js-cookie": "^2.2.0",
    "jshint": "^2.9.5",
    "keycode": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "nodemon": "^1.18.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
